FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application     SNOWPACKFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         5e9;

deltaT          5;//900;//10;

writeControl    adjustableRunTime;//timeStep;adjustableRunTime;

writeInterval   10800;//10;//10800;

purgeWrite      0; // last two tme steps

writeFormat     ascii; //ascii;binary;

writePrecision  7;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           5;

maxDeltaT 	250;//10;//900;


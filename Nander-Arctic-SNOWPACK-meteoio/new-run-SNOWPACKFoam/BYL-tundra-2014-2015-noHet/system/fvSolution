FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
solvers
{
    rho
    {
        solver          PBiCGStab;
        preconditioner  DIC;
        //solver          smoothSolver;
        //smoother        symGaussSeidel;                
        tolerance       1e-6;
        relTol          0;        
    }

    rhoFinal
    {
        $rho;
        //tolerance       1e-09;
        //relTol          0;
    }    

    p_rgh
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0.001;
/*	        
        solver           GAMG;
        smoother         GaussSeidel;//DICGaussSeidel;
        tolerance       1e-7;
        relTol          0.001;
*/
    }
    p_rghFinal
    {
        $p_rgh;
        relTol          0;
    }

    U
    {
        solver          PBiCGSta;
        preconditioner  DILU;
        //solver          smoothSolver;
        //smoother        symGaussSeidel;        
        tolerance       1e-6;
        relTol          0.0;
    }
    UFinal
    {
        $U;
    }

    Tg
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        //solver           GAMG;
        //smoother         DILUGaussSeidel;        
        //solver          smoothSolver;
        //smoother        symGaussSeidel;
        tolerance       1e-6;
        relTol          0;
    }
    TgFinal
    {
        $Tg;
    }
           
    Ti
    {
        solver          PBiCGStab;
        preconditioner  DIC;
        //solver          smoothSolver;
        //smoother        symGaussSeidel;
        tolerance       1e-6;
        relTol          0;
    }
    TiFinal
    {
        $Ti;
    }
           
    rho_v
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        //solver          smoothSolver;
        //smoother        symGaussSeidel;
        tolerance       1e-6;
        relTol          0;
    }
    rho_vFinal
    {
        $rho_v;
    } 
          
    voidFraction
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        //solver          smoothSolver;
        //smoother        symGaussSeidel;
        tolerance       1e-7;
        relTol          0.0;
    }
    voidFractionFinal
    {
        $voidFraction;
    }  
       
    iceFraction
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        //solver          smoothSolver;
        //smoother        symGaussSeidel;
        tolerance       1e-7;
        relTol          0.0;
    }
    iceFractionFinal
    {
        $iceFraction;
    }
             
    waterFraction
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        //solver          smoothSolver;
        //smoother        symGaussSeidel;
        tolerance       1e-7;
        relTol          0.0;
    }
    waterFractionFinal
    {
        $waterFraction;
    }    
}

PIMPLE
{
    momentumPredictor no;
    nOuterCorrectors 1;
    nCorrectors     1;
    nNonOrthogonalCorrectors 0;
    
    pRefCell        0;
    //pRefPoint       (0 0 0);
    pRefValue       101325;
    
    
    residualControl
    {
		"(U|p_rgh)"//"(U|p_rgh|Ti|Tg)"
		{
			relTol          0;
			tolerance      1.e-3;
		}
		"(Ti|Tg)"
		{
			relTol          0;
			tolerance      1.e-3;
		}
    }	  
}

